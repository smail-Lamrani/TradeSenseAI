# TradeSense Backend Environment Variables

# Flask
SECRET_KEY=tradesense-secret-key-change-in-production
FLASK_ENV=development
FLASK_DEBUG=1

# Database
DATABASE_URL=sqlite:///tradesense.db

# JWT
JWT_SECRET_KEY=jwt-secret-key-change-in-production

# CORS
CORS_ORIGINS=http://localhost:5173,http://localhost:3000

# Market Data
MARKET_UPDATE_INTERVAL=30

# PayPal (configure in SuperAdmin panel)
PAYPAL_MODE=sandbox
PAYPAL_CLIENT_ID=
PAYPAL_CLIENT_SECRET=
